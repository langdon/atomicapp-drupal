{
  "apiVersion": "v1beta1",
  "id": "mariadb",
  "desiredState": {
    "manifest": {
      "version": "v1beta1",
      "id": "mariadb",
      "containers": [
        {
          "name": "mariadb",
          "image": "localhost:5000/mariadb",
          "env": [
            {
              "name": "MYSQL_ROOT_PASSWORD",
              "value": "yourpassword"
            }
          ],
          "cpu": 1,
          "ports": [
            {
              "containerPort": 3306
            }
          ]
        }
      ]
    }
  },
  "labels": {
    "name": "mariadb"
  },
  "kind": "Pod"
}
